{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Web App (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:19006",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "yarn web",
      "sourceMaps": true,
      "trace": true,
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}
